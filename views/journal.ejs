
<%- include("header") -%>

<div class="neomorphism-card">
  <form class="inputEntry list-journal" action="/journal" method="post">
    <h6 class="list-journal-header-subject input-input-subject-title">Title</h6>
    <input class="list-subject-entry input-input-subject" type="text" name="subjectEntry" placeholder="Enter a title...">
    <h6 class="list-journal-title input-textarea-title">Journal Entry</h6>
    <textarea class="list-input-entry input-textarea" name="entry" rows="8" cols="80" placeholder="What are you grateful for?"></textarea>
    <button class="button-round list-button" type="submit" name="submitEntry">+</button>
  </form>

</div>


<%- include("footer") -%>
